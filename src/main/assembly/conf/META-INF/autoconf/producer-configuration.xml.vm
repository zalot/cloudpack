<?xml version="1.0"?>
<configuration>
	<property>
		<name>fs.default.name</name>
		<value>${replication_producer_fs}</value>
		<description>根路径</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.hbase.rootdir</name>
		<value>${replication_producer_hbase_rootdir}</value>
		<description>根路径</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.fs</name>
		<value>${replication_producer_fs}${replication_producer_fs_root}</value>
		<description>同步生产端的filesystem位置</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.tmpfile.targetPath</name>
		<value>${replication_tmpfile_targetPath}</value>
		<description>待处理的中间文件存放位置</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.tmpfile.oldPath</name>
		<value>${replication_tmpfile_oldPath}</value>
		<description>已处理的中间文件存放位置</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.tmpfile.rejectPath</name>
		<value>${replication_tmpfile_rejectPath}</value>
		<description>退回的中间文件存放位置（需要producer端重做）</description>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.tmpfile.tmpPath</name>
		<value>${replication_tmpfile_tmpPath}</value>
		<description>tmp文件存放位置</description>
	</property>
	<property>
		<name>hbase.zookeeper.quorum</name>
		<value>${replication_producer_zookeeper_quorum}</value>
	</property>
	<property>
		<name>hbase.zookeeper.property.clientPort</name>
		<value>${replication_producer_zookeeper_quorum_port}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.zoo.root</name>
		<value>${replication_producer_zoo_root}</value>
	</property>
	<!-- -->
	<property>
		<name>com.alibaba.hbase.replication.producer.hbase.rootdir</name>
		<value>${replication_producer_hbase_rootdir}</value>
		<description>根路径</description>
	</property>
	<!-- Thread Lock -->
	<property>
		<name>com.alibaba.hbase.replication.producer.zoo.lock.root</name>
		<value>${replication_producer_zoo_lock_root}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.zoo.lock.scan.sleeptime</name>
		<value>${replication_producer_zoo_lock_scan_sleeptime}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.zoo.lock.scan.retrytime</name>
		<value>${replication_producer_zoo_lock_scan_retrytime}</value>
	</property>
		<property>
		<name>com.alibaba.hbase.replication.producer.zoo.lock.reject.sleeptime</name>
		<value>${replication_producer_zoo_lock_reject_sleeptime}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.zoo.lock.reject.retrytime</name>
		<value>${replication_producer_zoo_lock_reject_retrytime}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.hlog.readerbuffer</name>
		<value>${replication_producer_hlog_readerbuffer}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.hlog.group.interval</name>
		<value>${replication_producer_hlog_group_interval}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.replicationPoolSize</name>
		<value>${replication_producer_replicationPoolSize}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.scannerPoolSize</name>
		<value>${replication_producer_scannerPoolSize}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.threadpool.queuesize</name>
		<value>${replication_producer_threadpool_queuesize}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.threadpool.keepAliveTime</name>
		<value>${replication_producer_threadpool_keepAliveTime}</value>
	</property>
	<property>
		<name>com.alibaba.hbase.replication.producer.threadpool.keepAliveTime</name>
		<value>${replication_producer_hlog_sleeptime}</value>
	</property>
	<property>
		<name>dfs.permissions</name>
		<value>false</value>
	</property>
</configuration>